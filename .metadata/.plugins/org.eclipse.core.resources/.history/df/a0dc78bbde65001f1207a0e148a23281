package test;

public class tes {
	public static void main(String[] args) {
		int num = 100;
		System.out.println(num);
		
		System.out.println(num + 30);
		System.out.println(num - 30);
		System.out.println(num * 30);
		System.out.println(num / 30);
		System.out.println(num % 30);
		
		System.out.println();
		System.out.println(num > 30);
		System.out.println(num <= 30);
		System.out.println(num == 30);
		
		char ch= 'ê°€';
		System.out.println(ch);
		System.out.println(++ch);
	}

}
