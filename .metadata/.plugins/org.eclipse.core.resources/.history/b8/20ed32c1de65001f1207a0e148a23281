package test;

public class tes {
	public static void main(String[] args) {
		int num = 100;
		System.out.println(num);
		
		System.out.println(num + 30);
		System.out.println(num - 30);
		System.out.println(num * 30);
		System.out.println(num / 30);
		System.out.println(num % 30);
		
		System.out.println();
		System.out.println(num > 30);
		System.out.println(num <= 30);
		System.out.println(num == 30);
		
		
		char ch= 'ê°€';
		System.out.println(ch);
		System.out.println(++ch);
		
		float f = 3.f;
		double d = 3.0;
		
		long l = 30L;
		
		byte b = 10;
		short s = 20;
		boolean b = true;
		
		System.out.println("f, d, l");
		
		
	}

}
